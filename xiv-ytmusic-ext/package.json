  "type": "module",
  "license": "MIT",
  "keywords": [
    "chrome-extension",
    "vanilla",
    "vite",
    "create-chrome-ext"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "protobuf": "protoc --plugin=protoc-gen-ts_proto=\".\\\\node_modules\\\\.bin\\\\protoc-gen-ts_proto.cmd\" \"--ts_proto_out=./src/gen\" \"--ts_proto_opt=env=browser\" \"--proto_path=../protobuf\" \"../protobuf/xivytmusic.proto\"",
    "dev": "vite",
    "build": "eslint . && pnpm run protobuf && tsc && vite build",
    "preview": "vite preview",
    "fmt": "eslint --fix .",
    "zip": "pnpm run build && node zip.ts"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.1.0",
