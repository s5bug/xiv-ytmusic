    "eslint": "^9.33.0",
    "jiti": "^2.5.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-zip-pack": "^1.2.4"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.6.3",
    "rxjs": "^7.8.2",
    "ts-proto": "^2.7.7"
  }
